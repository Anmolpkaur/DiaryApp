<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.anmol.diaryapp">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DiaryOptions" />
        <activity android:name=".Login" />
        <activity android:name=".SeeDiaryEntries" />
        <activity android:name=".NewDiaryEntry" />
        <activity android:name=".CreateToDo" />
        <activity android:name=".SeeToDo" />
        <activity android:name=".FavouritePeople" />
        <activity android:name=".SeeFavourites" />
        <activity android:name=".Update_favs" />
        <activity android:name=".DeletePeople"></activity>
    </application>

</manifest>